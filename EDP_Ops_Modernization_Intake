<script>
  document.getElementById("platform-ops-intake-form").addEventListener("submit", function (e) {
    e.preventDefault();

    const form = e.target;
    const fd = new FormData(form);

    const fields = [
      ["Organization", "company"],
      ["Industry", "industry"],
      ["Your Name", "name"],
      ["Your Role / Title", "role"],
      ["Email", "email"],
      ["Phone", "phone"],
      ["Top Platform Outcomes (6‚Äì12 months)", "platform_outcomes"],
      ["Key Constraints / Non-negotiables", "platform_constraints"],

      ["EDW System of Record Definition", "edw_definition"],
      ["Primary EDW Technology", "primary_db"],
      ["Hosting / Environment", "compute_hosting"],
      ["Major Source Systems and Inbound Feeds", "ingestion_sources"],
      ["ETL/ELT Tooling and Orchestration", "etl_tools"],
      ["Data Modeling Approach", "modeling_pattern"],
      ["Downstream Consumers and Delivery Patterns", "downstream_consumers"],

      ["Incident Frequency and Common Failure Modes", "incident_frequency"],
      ["Severity Model (Sev 1‚Äì3)", "severity_model"],
      ["On-call / Escalation Coverage", "oncall_model"],
      ["Monitoring and Alerting Approach", "monitoring_alerting"],
      ["Typical MTTR (Mean Time to Recovery)", "mttr"],
      ["Target RTO/RPO (if applicable)", "rto_rpo"],
      ["Runbooks / Triage Procedures", "runbooks"],
      ["Postmortems / Root Cause Analysis (RCA)", "postmortems"],

      ["Data Quality Definition and Metrics", "quality_definitions"],
      ["Where Quality Checks Are Implemented", "quality_checks"],
      ["Reconciliation and Controls", "reconciliation"],
      ["Data Quality Issue Workflow", "dq_issue_workflow"],

      ["Key External Partners and Data Exchanged", "partner_inventory"],
      ["Integration Patterns", "integration_patterns"],
      ["Common File Formats", "file_formats"],
      ["Data Contracts / Interface Specs", "data_contracts"],
      ["Validation and Acceptance Rules", "validation_rules"],
      ["Partner SLAs and Expectations", "partner_slas"],
      ["End-to-end Handling of Partner Failures", "partner_failures"],

      ["Sensitive or Regulated Data Present", "sensitive_data"],
      ["Access Control Model", "access_model"],
      ["Audit Logging", "audit_logging"],
      ["Encryption Standards", "encryption"],
      ["Retention and Deletion Practices", "retention"],
      ["Security or Privacy Incident History", "security_incidents"],

      ["Development Lifecycle and Release Process", "sdlc"],
      ["Prioritization Ownership", "backlog_ownership"],
      ["Capacity Planning Approach", "capacity_planning"],
      ["Technical Standards and Guardrails", "standards"],
      ["Testing Approach", "testing"],
      ["Documentation and Knowledge Management", "documentation"],

      ["Definition of Modernization for the Organization", "modernization_goals"],
      ["Preferred Cloud / Target Platform", "cloud_target"],
      ["Top Platform Automation Opportunities", "automation_opportunities"],
      ["AI Enablement Needs (Near Term)", "ai_enablement"],
      ["Cost and Performance Pain Points", "cost_performance"],

      ["Top Risks if Nothing Changes", "top_risks"],
      ["Quick Wins (60‚Äì90 days)", "quick_wins"],
      ["First Focus Area and Why", "first_focus"],
      ["Additional Notes", "additional_notes"]
    ];

    const headerRow = fields.map(([label]) => label);
    const valuesRow = fields.map(([_, key]) => fd.get(key) || "");

    const ws = XLSX.utils.aoa_to_sheet([headerRow, valuesRow]);

    const wb = XLSX.utils.book_new();
    XLSX.utils.book_append_sheet(wb, ws, "Platform Ops Intake");

    XLSX.writeFile(wb, "ClevelandStrategic_Enterprise_Data_Platform_Ops_Modernization_Intake.xlsx");

    alert("Thank you. Your responses have been downloaded as an Excel file.");
    form.reset();
  });
</script>
